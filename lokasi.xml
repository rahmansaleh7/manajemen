<odoo>
	<data>

		<record id="view_manajemen_lokasi_tree" model="ir.ui.view">
			<field name="name">manajemen.lokasi.tree</field>
			<field name="model">manajemen.lokasi</field>
			<field name="type">tree</field>
			<field name="priority" eval="8" />

			<field name="arch" type="xml">
				<tree string="Lokasi">
					<field name="no" />
					<field name="nolokasi" />
					<field name="name" />
				</tree>
			</field>
		</record>

		<record id="view_manajemen_lokasi_form" model="ir.ui.view">
			<field name="name">manajemen.lokasi.form</field>
			<field name="model">manajemen.lokasi</field>
			<field name="type">form</field>
			<field name="priority" eval="8" />

			<field name="arch" type="xml">
				<form string="Lokasi">
					<sheet>
						<div class="oe_title">
							<label for="name"
								string="Lokasi"/>
							<h1><field name="name"/></h1>
						</div>
						<group>
							<field name="no" />
							<field name="nolokasi" />
						</group>
					</sheet>
				</form>
			</field>
		</record>

	</data>
</odoo>