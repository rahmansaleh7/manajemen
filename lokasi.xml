<odoo>
	<data>

		<record id="view_manajemen_lokasi_tree" model="ir.ui.view">
			<field name="name">manajemen.lokasi.tree</field>
			<field name="model">manajemen.lokasi</field>
			<field name="type">tree</field>
			<field name="priority" eval="8" />

			<field name="arch" type="xml">
				<tree string="Lokasi">
					<field name="no" />
					<field name="nolokasi" />
					<field name="lokasi" />
					<field name="aksi" />
				</tree>
			</field>
		</record>

	</data>
</odoo>